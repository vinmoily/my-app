pipeline {
    agent any 
    stages {
        stage('SCM-Checkout') { 
            steps {
			    git branch: 'main', credentialsId: 'Git-Credential', 
                 url: 'https://github.com/vinmoily/my-app.git'
            }
        }
        stage('Build') { 
            steps {
			      echo " I am in build step" 
            }
        }
        stage('Test') { 
            steps {
			      echo " I am in Test step"
            }
        }
        stage('Deploy') { 
            steps {
			      echo " I am in Deploy step"
            }
        }			
    }
}
